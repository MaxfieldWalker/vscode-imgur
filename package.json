{
  "name": "vscode-imgur",
  "displayName": "vscode-imgur",
  "description": "imgur integration for vscode",
  "version": "1.0.0",
  "publisher": "MaxfieldWalker",
  "galleryBanner": {
    "color": "#000",
    "theme": "dark"
  },
  "icon": "",
  "repository": {
    "type": "git",
    "url": "https://github.com/MaxfieldWalker/vscode-imgur"
  },
  "bugs": {
    "url": "https://github.com/MaxfieldWalker/vscode-imgur/issues"
  },
  "homepage": "",
  "license": "MIT",
  "engines": {
    "vscode": "^1.5.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:vscode-imgur.pasteImage"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "commands": [
      {
        "command": "vscode-imgur.pasteImage",
        "title": "Paste Image"
      }
    ],
    "keybindings": [
      {
        "command": "vscode-imgur.pasteImage",
        "key": "ctrl+alt+v",
        "mac": "cmd+alt+v",
        "when": "editorTextFocus"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.41",
    "@types/node": "^7.0.22",
    "mocha": "^3.4.2",
    "typescript": "^2.3.3",
    "vscode": "^1.1.0"
  },
  "dependencies": {
    "@rmp135/imgur": "^0.4.0"
  }
}